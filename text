// Tapşırıq 1
console.log(5 > 3 && 4 < 6); // true
console.log(10 == 10 || 7 < 5); // true
console.log(!(8 != 8)); // true
console.log(3 >= 3 && 2 == 2); // true
console.log(!(5 < 2 || 9 > 10)); // true

// Tapşırıq 2
let a = 7;
let b = 5;
let result = (a > b) && (b != 5);
console.log(result); // false

let x = 10;
let y = 15;
let check = !(x == y || x > y);
console.log(check); // true

// Tapşırıq 3
a = 4;
b = 2;
let c = 6;
result = !(a > b && c < a);
console.log(result); // true